{
  "name": "@elizaos/plugin-autonomous",
  "version": "0.1.0",
  "description": "Autonomous operation plugin for Eliza OS",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "@elizaos/source": "./src/index.ts",
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/*.ts --format esm --dts",
    "dev": "tsup src/*.ts --format esm --dts --watch",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "22.8.4",
    "tsup": "8.3.5",
    "typescript": "5.6.3",
    "vitest": "^1.4.0"
  },
  "dependencies": {
    "@elizaos/core": "workspace:*"
  },
  "publishConfig": {
    "access": "public"
  }
} 